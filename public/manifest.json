{
  "manifest_version": 3,
  "name": "Prompt Manager",
  "version": "1.0.1",
  "description": "A prompt manager for artificial intelligences.",
  "permissions": [
    "storage",
    "clipboardWrite",
    "unlimitedStorage"
  ],
  "action": {
    "default_popup": "index.html",
    "default_title": "Prompt Manager"
  },
  "icons": {
    "16": "./assets/PromptManager.16.png",
    "48": "./assets/PromptManager.48.png",
    "64": "./assets/PromptManager.64.png",
    "128": "./assets/PromptManager.128.png"
  }
}
